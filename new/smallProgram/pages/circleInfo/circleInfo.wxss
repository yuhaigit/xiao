.gotoCireBtn{
    width:100%;
    height:120rpx;
    background-color:#fff;
    position:fixed;
    bottom:0;
    left:0;
    padding:0 30rpx;
    box-sizing:border-box;
    padding-top:13rpx;
}
.gotoCireBtns{
    background-color: #ff6666 !important;
    color: #fff!important;
    display: block;
    width: 100%;
    height: 96rpx!important;
    line-height: 96rpx!important;
    text-align: center;
    font-size: 32rpx!important;
    border-radius: 48rpx;
    background: -webkit-linear-gradient(left, #EA5C5C , #D02F2F)!important;
}
button::after{
    border: none !important;
}
  


