@import "../../utils/iconfont.wxss";

.iconfont{
    font-size: 36rpx;
}
.icon_wodeshoucang{
    font-size: 48rpx;
    color: #999999;
}


.CircleListBox{
    width: 100%;
    margin-top:120rpx;

}
.CircleListBox .CircleList{
    width: 100%;
}
.CircleListBox .CircleListImg{
    position: relative;
}
.CircleListBox .CircleListImg .imgs{
    width: 164rpx;
    height: 96rpx;
    margin-right: 10rpx;
}
.CircleListBox .CircleListImg .userName{
    position: absolute;
    top: 0;
}
.CircleListBox .CircleListImg .userTime{
    position: relative;
    top: -5rpx;
}
.van-cell__right-icon-wrap{
    position: relative;
    top: 10rpx;
}

.TopBox{
    width: 100%;
    position: relative;
}
.TopBox .imgs{
    width: 100%;
    height: 440rpx;
}
.TopBox .plays{
    width:120rpx;
    height:120rpx;
    position:absolute;
    left:0;
    top:140rpx;
    margin:auto;
    right:0;
    z-index:99;
}
.TopBox .CircleInfos{
    width: 100%;
    height:140rpx;
    background-color: #ffffff;
    padding: 12rpx 30rpx;
    box-sizing: border-box;
    margin-top:-10rpx;
}
.TopBox .CircleInfos .title{
    font-size: 36rpx;
    color: #000000;
}
.TopBox .CircleHead{
    width: 100%;
    position: relative;
    top:13rpx;
}
.TopBox .CircleHead image{
    width: 48rpx;
    height: 48rpx;
    border-radius: 50%;
    float: left;
    position: absolute;
}
.TopBox .CircleHead image:nth-child(2){
    left: 20rpx
}
.TopBox .CircleHead image:nth-child(3){
    left: 40rpx
}
.TopBox .CircleHead image:nth-child(4){
    left: 60rpx
}
.TopBox .CircleHead .CircleNum{
    float: left;
    font-size: 30rpx;
    color: #999999;
    position: relative;
    top: 5rpx;
    left: 14%;
}

.page-bodys{
    width: 100%;
    /* height: ; */
    background-color: #F6F6F6;
}
/* 简介 */
.introduce {
    width: 100%;
    /* height: 300rpx; */
    padding: 10rpx 30rpx;
    box-sizing: border-box;
    margin: 20rpx 0;
    background-color: #ffffff;
}
.introduces {
    margin-top: 0;
    height: auto;
}
.introduce .title{
    width: 100%;
    height: 96rpx;
    font-size:36rpx;
    color: #333333;
    line-height:96rpx;
    font-weight:400;
}
.introduce .introduceBox{
    width: 100%;
    margin-top:20rpx;
}
.introduce .introduceBox2{
    width: 100%;
    margin-top:0;
}
/* 富文本解析 */
.wxmlified-image {
    width: 100%;
}


.introduce .introduceBox .intro_top{
    width: 100%;

}
.introduce .introduceBox .headImg{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;

}
.introduce .introduceBox .sexs{
    width: 18rpx;
    height: 18rpx;
    border-radius: 50%;
    position: relative;
    left: -10rpx;
}
.introduce .introduceBox .intro_name{
    font-size:28rpx;
    color: #333333;
    position: relative;
    top: -20rpx;
    font-weight:400;
}
.introduce .introduceBox .btn{
    width:120rpx;
    height:46rpx;
    background-color:#fff;
    border-radius:30rpx;
    padding:unset;
    font-size:28rpx;
    color:#D02F2F;
    text-align:center;
    line-height:43rpx;
    float:right;
    border:2rpx solid #ea5c5c;
}
.introduce .introduceBox .btns{
    width:120rpx;
    height:48rpx;
    background:linear-gradient(141deg,rgba(234,92,92,1) 0%,rgba(208,47,47,1) 100%);
    border-radius:30rpx;
    padding: unset;
    font-size: 28rpx;
    color: #FFFFFF;
    text-align: center;
    line-height: 48rpx;
    float: right;
    padding: unset;
}


.introduce .introduceBox .intro_bottom{
    width:100%;
    padding-left:72rpx;
    box-sizing:border-box;
}
.introduce .intro_bottomText{
    text-overflow: ellipsis;
    white-space: normal;
    /* overflow: hidden; */
    display: -webkit-box;
    line-height:37rpx;
    /* -webkit-line-clamp: 2; */
    /* -webkit-box-orient: vertical; */
    font-size: 26rpx;
    color: #333333;
    font-weight:400;
}

.introduce .intro_bottomTexts{
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: unset;
    -webkit-box-orient: vertical;
    font-size: 26rpx;
    color: #333333;
}


