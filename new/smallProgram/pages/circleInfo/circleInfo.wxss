.studyBox{
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background-color: #FFFFFF;
}
.studyBox .CircleList{
    width: 100%;  
}
.studyBox .title{
    width:100%;
    height:96rpx;
    font-size:36rpx;
    color:#333333;
    padding:0 30rpx;
    box-sizing:border-box;
    line-height:96rpx;
    font-weight:400;
}
/* .studyBox .courseName{
    width: 100%;  
} */
.van-cell--clickable.van-cell--hover {
    background-color:#FFF !important;
}
.courseNum{
    color: #999999;
    font-size:24rpx;
}
.courseName{
    display: block;
    color: #333333 !important;
    font-size:28rpx !important;
    font-weight: 400;
}
.van-cell__right-icon-wrap{
    position: relative !important;
    top: 25rpx  !important;
}

/* 打卡 */
.cireCard{
    width: 100%; 
    background-color: #FFFFFF;
    margin-top:20rpx;
}
.cireCard .title{
    width:100%;
    height:96rpx;
    font-size:36rpx;
    color:#333333;
    padding:0 30rpx;
    box-sizing:border-box;
    line-height:96rpx;
    font-weight:400;
}
.cireCard .nodata{
    text-align:center;
    width:100%;
} 
.cireCard .lists{
    width: 100%;
    padding: 24rpx 30rpx;
    box-sizing: border-box;
    background-color:#f6f6f6;
} 
.cireCard .CardDiaryInfos{
    width: 100%;
    /* height: 995rpx; */
    position: relative;
    overflow: hidden;
    border-radius:12rpx;
    border:1rpx solid rgba(216,216,216,1);
    margin-bottom:20rpx;
    background-color:#fff;
}
.cireCard .imgs{
    width: 100%;
    /* height: 897rpx;  */
}
.cireCard .likes{
    display: inline-block;
    color: #333333;
    position: absolute;
    top:10rpx;
    right:11rpx; 
}
.cireCard .goodListInfos{
    width:100%;
    padding:10rpx 15rpx;
    box-sizing:border-box;
    padding-right:0;
    display:inline-block;
    background-color:#fff;
}
.goodListInfos .userInfos_l{
    display: inline-block;
    width: 65%;
    float: left;
    padding-right: 0;
    box-sizing: border-box;
}
.goodListInfos .userInfos_l navigator{
    display: inline-block;
    width: 100%;
    height: 100%;
    vertical-align: top;
}
.goodListInfos .userHeads{
    float: left;
}
.goodListInfos .userHead{
    width: 72rpx;
    height: 72rpx;
    border-radius: 50%;
    float: left;
}
.goodListInfos .userSex{
    width: 18rpx;
    height: 18rpx;
    border-radius: 50%;
    float: left;
    position: relative;
    top:52rpx;
    left:-10rpx;
}
.goodListInfos .userName{
    width:70%;
    font-size:24rpx;
    color: #333333;
    float: left;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.goodListInfos .userTime{
    /* width:60%; */
    font-size:22rpx;
    color: #999999;
    float: left;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    margin-top:8rpx;
}
.goodListInfos .userHandle{
    float: right;
    display: flex;
    justify-content: space-around;
    align-items:center;
    height:100%;
    margin-top:28rpx;
}
.goodListInfos .zan{
    color:#999999;
    font-size:22rpx;   
}
.goodListInfos .zan .icons{
    position: relative;
    top:8rpx;
}
.goodListInfos .reply{
    color: #999999;
    font-size:22rpx;
}
.goodListInfos .zan ,.goodListInfos .reply{
    padding: 0 15rpx;
    box-sizing: border-box;
}
.goodListInfos .zan .nums,.goodListInfos .reply .nums{
    padding: 0 5rpx;
    box-sizing: border-box;
}



.CardDiary .CardDiaryUser{
    width: 100%;
    height: 188rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: #FFFFFF;
    padding: 12rpx 24rpx;
    box-sizing: border-box;
}

.gotoCire{
    width: 100%;
    text-align: center;
    position: relative;
    margin-bottom:150rpx;
    font-size:28rpx;
    color: #999999;
}
