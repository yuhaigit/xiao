.content{
    width: 100%;
    background-color:#F6F6F6;
}
.container{
    width: 100%;
}

/* 顶部信息 */
.container .TopuserBox{
    width: 100%;
    height:260rpx ;
    background-color: #fff;
    display: inline-block;
    padding-top: 30rpx;
    box-sizing: border-box;
}
.content .TopuserBox view{
    display: inline-block;
}
.container .TopuserBox .topUser{
    width: 100%;
    float: left;
    padding: 0 30rpx;
    box-sizing: border-box;
}
.container .TopuserBox .topUser .imgs{
    width: 84rpx;
    height: 84rpx;
    border-radius: 50%;
    float: left;
    margin-right: 10rpx;
}
.container .TopuserBox .topUser .UserInfos{
    float: left;
    width: 60%;
}
.container .TopuserBox .topUser .UserInfos button{
    background-color:unset !important;
    border:unset!important;
    font-size:36rpx!important;
    color:#333!important;
    display:inline-block;
    line-height:unset;
    padding:unset;
    margin:unset;
    float:left;
}
.container .TopuserBox .topUser .UserInfos button::after{
    border:none !important;
}
.container .TopuserBox .topUser .UserInfos .userUame{
    max-width:90%;
    font-size:36rpx;
    color: #333333;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    float:left;
}
.container .TopuserBox .topUser .UserInfos .sexs{
    width: 26rpx;
    height: 26rpx;
    float: left;
    position: relative;
    top: 14rpx;
}
.container .TopuserBox .topUser .UserInfos .userTime{
    float:left;
    width:100%;
    font-size:24rpx;
    color: #999999;
    display:block;
    /* margin-top:10rpx; */
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.container .TopuserBox .topUser .goUserInfos{
    float:right;
    width:20%;
    height: 100%;
	padding-top:32rpx;
}
.container .TopuserBox .topUser .goUserInfos navigator{
    display: inline-block;
    font-size:24rpx;
    font-weight:400;
	color: #D8D8D8;
	float:right;

}
.container .TopuserBox .topUser .goUserInfos  .iconss{
	position: relative;
	top: 5rpx;
}
.container .TopuserBox .topMenu{
    width: 100%;
    float:left;
    display:flex;
    justify-content:space-around;
    align-items:center;
    border-top:1rpx solid #F6F6F6;
    padding:10rpx 30rpx;
    margin-top:20rpx;
    box-sizing:border-box;
	padding-top:20rpx; 
}
.container .TopuserBox .topMenu .menu{
    text-align:center;
}
.container .TopuserBox .topMenu .menu text:first-child{
    font-size:30rpx;
    color: #333333;
	display:block;
	font-weight:400;
}
.container .TopuserBox .topMenu .menu text:last-child{
    font-size:22rpx;
    color: #999999;
    position: relative;
    top: -10rpx;
}

/* 列表 */
.ListBox{
	width: 100%;
	margin-top: 10rpx;
}
.van-cell--clickable.van-cell--hover {
    background-color:#fff !important;
}
    
.ListBox .lists .van-cell__title{
	color: #333333;
	font-weight:400;
}
.ListBox .lists .van-icon{
	color: #D8D8D8;
}

/* 打卡 */
.goCard{
	width:100%;
	background-color:#F6F6F6;
	text-align:center;
	display:inline-block;
	box-sizing:border-box;
	height:500rpx;
    position: relative;
    line-height: 500rpx;
    display: inline-block;
	font-size:30rpx;
	font-weight:bold;
    color: #999999;
    text-decoration: underline;
}
