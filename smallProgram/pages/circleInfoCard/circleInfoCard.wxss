.mapsBox .mapsLists .map_title{
    width: 100%;
    height: 110rpx;
    background-color: #ffffff;
    padding: 0 30rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #333333;
    font-weight: 400;
    line-height: 110rpx;
    position: relative;
}
.mapsBox .mapsLists .map_title .lines{
    width:64rpx;
    height:6rpx;
    background:rgba(216,216,216,1);
    border-radius:10rpx;
    display:inline-block;
    position:absolute;
    margin:auto;
    left:0;
    right:0;
    top:-70rpx;
    bottom:0;
}
.mapsBox .mapsLists .map_title .nums{
    font-size: 36rpx;
    color: #333333;
    font-weight: 400;
}
.mapsBox .mapsLists .map_list{
    display:flex;
    justify-content:space-between;
    background-color:#f6f6f6;
    flex-wrap:wrap;
    padding:20rpx 30rpx;
    box-sizing:border-box;
}
.mapsBox .mapsLists .map_list .navi{
    display:flex;
    flex:0 0 48%;
    flex-wrap:wrap;    
}
.mapsBox .mapsLists .map_list .maps_list_info{
    height:114rpx;
    background-color:#ffffff;
    position:relative;
    display:flex;
    /* flex:0 0 48%; */
    flex-wrap:wrap;
    margin-bottom:20rpx;
    padding:20rpx 18rpx 0 18rpx;
    box-sizing:border-box; 
    border-radius:10rpx;
    overflow: hidden; 
}
.mapsBox .mapsLists .map_list .maps_list_info .maps_list_infoImg{
    height:100%;
    display:inline-blcok;
    width:80rpx;
}
.mapsBox .mapsLists .map_list .maps_list_info .headImg{
    width: 75rpx;
    height: 75rpx;
    border-radius: 50%;
    overflow: hidden;

}
.mapsBox .mapsLists .map_list .maps_list_info .sexImg{
    width: 18rpx;
    height: 18rpx;
    border-radius: 50%;
    overflow: hidden;
    position:absolute;
    left:72rpx;
    top:78rpx;
}
.mapsBox .mapsLists .map_list .maps_list_info .userName{
    width:150rpx;
    height:40rpx;
    font-size:28rpx;
    color: #333333;
    font-weight:400;
    margin-left:12rpx;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;       word-wrap:break-word;
    word-break:break-all;
}
.mapsBox .mapsLists .map_list .maps_list_info .distance{
    width:150rpx;
    height:40rpx;
    font-size:22rpx;
    color: #999999;
    font-weight:400;
    position:relative;
    top:-30rpx;
    left:90rpx; 
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical; 
    word-break:break-all;
    word-wrap:break-word;
}
