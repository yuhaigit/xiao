@import "../../utils/iconfont.wxss";

/* 顶部导航栏 */
page{
    background-color:#f6f6f6;
}
.content{
    width: 100%;
}

.container{
    width: 100%;
    height:100%;
    background-color:#F6F6F6;
}

.iconfont{
    font-size: 36rpx;
}
.icon_wodeshoucang{
    font-size: 48rpx;
    color: #999999;
}

.CircleListBox{
    width: 100%;
}
.CircleListBox .CircleList{
    width: 100%;
}
.CircleListBox .CircleListImg{
    position: relative;
}
.CircleListBox .CircleListImg .imgs{
    width: 164rpx;
    height: 96rpx;
    margin-right: 10rpx;
}
.CircleListBox .CircleListImg .userName{
    position: absolute;
    top: 0;
}
.CircleListBox .CircleListImg .userTime{
    position: relative;
    top: -5rpx;
}
.van-cell__right-icon-wrap{
    position: relative;
    top: 10rpx;
}


/* 页面内容 */
.page-bodys{
    width: 100%;
    background-color: #F6F6F6;
}
.pageConent{
    width: 100%;
    position: relative;
}
.pageConent .courseImgs{
    width: 100%;
    height: 400rpx;
}
.pageConent .plays{
    width:120rpx;
    height:120rpx;
    position:absolute;
    left:0;
    top:140rpx;
    margin:auto;
    right:0;
    z-index:99;
}
.videoBox{
  display: inline-block;
  width: 100%;
}




/* 全部练习 */
.allStudy{
    width: 100%;
    background-color:#f6f6f6;
    box-sizing: border-box;
    display:inline-block;
    position:relative;
    top:-8rpx;
}
.allStudy .titles{
    width: 100%;
    height:50rpx;
    font-size:30rpx;
    color: #999999;
    float: left;
}
.nodata{
    width: 100%;
    text-align: center;
}
.allStudy .allList {
    width: 100%;
    display:inline-block;
    height:100%;
}
.allStudy .allList .lists{
    width:100%;
    height:109rpx;
    font-weight:400;
    background-color: #fff;
    display:inline-block;
    box-sizing:border-box;
    padding:0rpx 30rpx;
    margin-bottom:12rpx;
}
.allStudy .allList .lists .all_num{
    font-size:24rpx;
    color: #999999;
    display:inline-block;
    width:100%;
    font-weight:400;
}
.allStudy .allList .lists .all_name{
    font-size:28rpx;
    color: #333333;
    margin-top:8rpx;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.allStudy .listsInfos {
    width:100%;
    background-color:#fff;
    display:inline-blcok;   
    float:left; 
    padding: 22rpx 30rpx;
    box-sizing: border-box;
}

/* 打卡日记 */
.cardDiary{
    width:100%;
    margin-top: 20rpx;
    height:300rpx ;
    background-color: #ffffff;
}

/* 新增 */
.listHeights{
    width: 100%;
    background-color:#f6f6f6;
    margin-top: 15rpx;
    padding-bottom:120rpx;
}
.listHeights .titles{
    width:100%;
    height:96rpx;
    font-size:36rpx;
    color:#333333;
    line-height:96rpx;
    padding:0 30rpx;
    font-weight:500;
    box-sizing:border-box;
    background-color:#fff;
    display:inline-block;
    margin-bottom:20rpx;
}
.listHeights .list{
    width:100%;
    padding:0 30rpx;
    box-sizing:border-box;
    background-color:#f6f6f6;
}
.listHeights .cireCard{
    width: 100%; 
    background-color: #FFFFFF;
    border-radius:12rpx;
    border:1rpx solid rgba(216,216,216,1);
    margin-bottom: 15rpx;
    overflow:hidden;
}
.cireCard view{
    display: inline-block;
}
.cireCard .title{
    width: 100%;
    height: 76rpx;
    font-size:30rpx;
    color: #999999;
    padding: 0 30rpx;
    box-sizing: border-box;  
}
.cireCard .CardDiaryInfos{
    width: 100%;
    /* height: 995rpx; */
    position: relative;
    overflow: hidden;
}
.cireCard .imgs{
    width: 100%;
}
.cireCard .likes{
    display: inline-block;
    color: #333333;
    position: absolute;
    top:11rpx;
    right:12rpx;  
}
.cireCard .PictureNum{
    width:63rpx;
    height:28rpx;
    display: inline-block;
    color: #EFEFEE;
    font-size:22rpx;
    font-weight:500;
    text-align: center;
    line-height: 28rpx;
    background:rgba(0,0,0,0.6);
    border-radius:14rpx;
    position: absolute;
    top:395rpx;
    right:10rpx;   
}
.cireCard .goodListInfos{
    width:100%;
    height:90rpx;
    overflow:hidden;
    padding:0rpx 16rpx;
    box-sizing:border-box;
    padding-top:10rpx;
}
.goodListInfos .userInfos_l{
    display: inline-block;
    width: 65%;
    float: left;
    padding-right: 0;
    box-sizing: border-box;
}
.goodListInfos .userHeads{
    float: left;
    /* margin-right: 15rpx; */
}
.goodListInfos .userHead{
    width: 74rpx;
    height: 74rpx;
    border-radius: 50%;
    float: left;
}
.goodListInfos .userSex{
    width: 18rpx;
    height: 18rpx;
    border-radius: 50%;
    float: left;
    position: relative;
    top:52rpx;
    left:-10rpx;
}
.goodListInfos .userName{
    width:73%;
    font-size:30rpx;
    color: #333333;
    float: left;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.goodListInfos .userInfos_l navigator{
    width:100%;
    height:100%;
    display:inline-block;
    vertical-align:top;
}
.goodListInfos .userInfos_l .userTimes{
    width:73%;
    font-size:24rpx;
    color: #999999;
    float: left;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    margin-top:3rpx;
}
.goodListInfos .userHandle{
    float: right;
    display: flex;
    justify-content: space-around;
    align-items:center;
    height:100%;
    padding-top:10rpx;
}
.goodListInfos .zan{
    color:#999999;
    font-size:22rpx;
}
.goodListInfos .zan,.goodListInfos .reply{
    padding: 0 15rpx;
    box-sizing: border-box;
}
.goodListInfos .zan .clickZan,.goodListInfos .reply .nums{
    position:relative;
    top:-3rpx;
    padding: 0 5rpx;
    box-sizing: border-box;
}
.goodListInfos .reply{
    color:#999999;
    font-size:36rpx;  
}

.goodListInfos .reply .nums{
    color:#999999;
    font-size:22rpx;  
}


