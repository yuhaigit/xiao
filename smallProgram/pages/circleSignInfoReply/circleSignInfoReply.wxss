.content{
    width: 100%;
    height:100%; 
    background-color:#F6F6F6;
    padding: 24rpx 30rpx;
    padding-bottom: 110rpx;
    box-sizing: border-box;
}

page{
    background-color: #F6F6F6;
}
.textBox{
    width: 100%;
    min-height:320rpx;
}
.textHeight{
    min-height:320rpx !important;
    background:rgba(255,255,255,1);
    border-radius:12rpx;
}

.Menu{
    width: 100%;
    height:96rpx ;
    display: flex;
    position: fixed;
    bottom: 0;
    left: 0;
    text-align: center;
    line-height: 96rpx;
    font-weight:400;
    font-size:32rpx;
    z-index: 999999;
}
.Menu .cancels{
    width: 35%;
    height:96rpx ;
    color: #d8d8d8;
    background-color: #ffffff;
}
.Menu .confirms{
    width: 65%;
    height:96rpx ;
    color: #999999;
    background-color: #d8d8d8;
}
.Menu .reds{
    color: #FFFFFF;
    background-color: #D02F2F;
}






