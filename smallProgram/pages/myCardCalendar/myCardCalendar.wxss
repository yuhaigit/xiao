@import "../../utils/iconfont.wxss";
page{
    background-color:#f6f6f6;
}
.container{
    width: 100%;
    height:100%; 
    background-color:#F6F6F6;
}
.iconfont{
    font-size: 36rpx;
}
.icon_wodeshoucang{
    color: #999999;
}


/* 顶部信息 */
.container .TopuserBox{
    width: 100%;
    height:265rpx ;
    background-color: #fff;
    display: inline-block;
    padding-top: 30rpx;
    box-sizing: border-box;
}
/* .content .TopuserBox view{
    display: inline-block;
} */
.container .TopuserBox .topUser{
    width: 100%;
    float: left;
    padding: 0 30rpx;
    box-sizing: border-box;
}
.container .TopuserBox .topUser .imgs{
    width: 84rpx;
    height: 84rpx;
    border-radius: 50%;
    float: left;
    margin-right: 10rpx;
}
.container .TopuserBox .topUser .UserInfos{
    float: left;
    width: 70%;
}
.container .TopuserBox .topUser .UserInfos button{
    background-color:unset !important;
    border:unset!important;
    font-size:36rpx!important;
    color:#333!important;
    display:inline-block;
    line-height:unset;
    padding:unset;
    margin:unset;
    float:left;
}
.container .TopuserBox .topUser .UserInfos button::after{
    border:none !important;
}
.container .TopuserBox .topUser .UserInfos button::after{
    border:none !important;
}

.container .TopuserBox .topUser .UserInfos .userUame{
    width:80%;
    font-size:36rpx;
    color: #333333;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    float:left;
}
.container .TopuserBox .topUser .sexs{
    width: 26rpx;
    height: 26rpx;
    float: left;
    position:relative;
    top:55rpx;
    right:30rpx;
}
.container .TopuserBox .topUser .UserInfos .userTime{
    float:left;
    width:100%;
    font-size:24rpx;
    color: #999999;
    display:block;
    margin-top:10rpx;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.container .TopuserBox .topMenu{
    width: 100%;
    float:left;
    display:flex;
    justify-content:space-around;
    align-items:center;
    border-top:1rpx solid #F6F6F6;
    padding:10rpx 30rpx;
    margin-top:20rpx;
    box-sizing:border-box;
    padding-top:20rpx;    
}
.container .TopuserBox .topMenu .menu{
    text-align:center;
}
.container .TopuserBox .topMenu .menu text:first-child{
    font-size:30rpx;
    color: #333333;
    display:block;
    position:relative;
    left:-5rpx;
}
.container .TopuserBox .topMenu .menu text:last-child{
    font-size:22rpx;
    color: #999999;
    position: relative;
    top: -10rpx;
}

/* 我的打卡日历 */
@import '../../template/calendar/index.wxss';
.calendar{
    width: 100%;
    margin-top:14rpx;
}
.calendar .calendarBox .van-collapse-item__content{
    padding: 0;
}
.calendar .calendarBox .van-cell__title{
    font-size:36rpx;
    color: #333333; 
}
.van-cell{
    height: 96rpx;
    line-height:60rpx !important;
}
.van-cell::after{
    border-bottom: 1px solid #fff !important;
}
.van-cell--clickable.van-cell--hover {
    background-color:#fff !important;
}
.van-cell__right-icon-wrap{
    position:relative;
    top:10rpx;
}
    
.todo-dot{
    width:58rpx;
    height:58rpx;
    border-radius:50%;
    background-color:unset;
    position:absolute;
    border:2rpx solid #d02f2f;
}
.unShow{
    height:650rpx;
    width:100%;
    display:inline-block;
}
/* 打卡信息 */
.CardInfos{
    display:inline-block;
    width:100%;
    height:100%;  
    margin-top:24rpx;   
}
.CardDiary{
    width: 100%; 
    /* height:800rpx; */
    padding: 0 30rpx;
    box-sizing: border-box;
}
.Circletext{
    font-size:36rpx;
    font-weight:400;
    color:#333333;
    width:100%;
    height:96rpx;
    background-color:#fff;
    display:inline-block;
    line-height:96rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
}
.CardDiary .CardDiaryInfos{
    width: 100%;
    border-radius:12rpx;
    background-color: #fff;
    border:1px solid rgba(216,216,216,1);
    position: relative;
    overflow: hidden;
    margin-top:20rpx;
}
.CardDiaryInfos .CardDiaryUserTop {
    width: 100%;
    height:342rpx;
    position: relative;
}
.CardDiaryInfos .CardDiaryUserTop .imgs{
    width: 100%;
    height:342rpx;
}
.CardDiary .CardDiaryInfos .shares{
    width:48rpx;
    height:48rpx;
    background:rgba(51,51,51,0.6);
    position: absolute;
    top:10rpx;
    right:10rpx;    
    font-size:18rpx;
    color: #FFFFFF;
    text-align: center;
    line-height: 48rpx;
    border-radius: 50%;
    padding:0;
}
.CardDiaryInfos .courseNames{
    width:100%;
    height:60rpx;
    line-height:60rpx;
    position:absolute;
    left:0;
    bottom:0;
    font-size:30rpx;
    color:#ffffff;
    background:rgba(0,0,0,0.6);
    text-align:center;    
}
.CardDiaryInfos .courseNames .courseTexts{
    width: 100%;height: 60rpx;
    font-size:30rpx;
    color: #ffffff;
    text-align: left;
    padding: 0 23rpx;
    box-sizing: border-box;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}


.goodListInfos .userInfos_l{
    line-height:50rpx;
    height:50rpx;
    display: inline-block;
    width: 100%;
    float: left;
    padding-right: 0;
    box-sizing: border-box;
}
.goodListInfos .userInfos_l .userTimes{
    float: left;
    font-size:28rpx;
    color: #999999;
    font-weight:400;
}
.goodListInfos .userInfos_l .userHandle{
    float: right;
}
.setZan ,.reply{
    padding: 0 20rpx;
    box-sizing: border-box;
}
.setZan .clickZan,.reply .nums{
    padding: 0 5rpx;
    box-sizing: border-box;
}
.goodListInfos .userInfos_l .setZan{
    float: left;
    font-size:28rpx;
    color: #999999;
    font-weight:400;
}
.goodListInfos .userInfos_l .reply{
    float: left;
    font-size:28rpx;
    color: #999999;
    font-weight:400;
}
.goodListInfos .userInfos_l .setGoodBox{
    float: right;
    margin-left: 15rpx;
}
.goodListInfos .userInfos_l .setGoodBox .icons{
    position: relative;
    top:5rpx;
}
.goodListInfos .userInfos_l .setGood{
    font-size:28rpx;
    color: #999999;
    font-weight:400;
    position: relative;
    top: -5rpx;
}



.goodListInfos .userHeads{
    float: left;
    margin-right: 15rpx;
}
.goodListInfos .userHead{
    width: 72rpx;
    height: 72rpx;
    border-radius: 50%;
    float: left;
}
.goodListInfos .userSex{
    width: 18rpx;
    height: 18rpx;
    border-radius: 50%;
    float: left;
    position: relative;
    top:52rpx;
    left:-10rpx;
}
.goodListInfos .userName{
    width:70%;
    font-size:30rpx;
    color: #333333;
    float: left;
    font-weight:500;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.goodListInfos .userTime{
    width:70%;
    font-size:24rpx;
    color: #999999;
    float: left;
    margin-top:5rpx;
    text-overflow: ellipsis;
    white-space: normal;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
/* 点赞人数 */
.goodListInfos .zan{
    width: 100%;
    height:142rpx ;
    background-color:#F6F6F6;
    border-radius:12rpx;
    margin-top:20rpx;
    color: #D8D8D8;
    font-size:22rpx;
    display: inline-block;
    padding: 10rpx 0;
    box-sizing: border-box;
} 
.goodListInfos .zan .zanNum{
    width: 90rpx;
    height: 100%;
    float: left;
    text-align:center;
    display: inline-block;
    padding-top:13rpx;
    box-sizing:border-box;
}
.goodListInfos .zan .zanNum text{
    color: #D02F2F;
    /* font-size: 28rpx; */
    display:block;
}
.goodListInfos .zan .zanImgs{
    width:85%;
    display:inline-block;
    float:left;   
    height: 100%;
    overflow: hidden; 
    padding:0 20rpx;
    box-sizing:border-box;    
}
.goodListInfos .zan .zanImgs navigator{
    display:inline-block;
    border-radius:50%;
    float:left;    
    margin-right:15rpx;
    margin-top:5px;
}
.goodListInfos .zan .zanImgs .imgs{
    width:48rpx;
    height:48rpx;
    border-radius: 50%;
    float: left;
}

